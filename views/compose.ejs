<%- include("partials/header") -%>

<h1> Compose </h1>

<form id="composeTitle" action="/compose" method="post">
  <div class="form-group">
    <label for="composeTitle">Title</label>
    <input type="text" name="contentTitle" id="compose" class= "form-control">
  </div>
  <div class="form-group">
    <label for="composePost"> Post</label>
    <textarea class= "contentPost form-control" name="contentPost" rows="5" cols="80"></textarea>
  </div>
  <button type="submit" name="button" class="btn btn-primary"> Publish </button>
</form>

<%- include("partials/footer") -%>
